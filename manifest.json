{
    "manifest_version": 2,
    "name": "Hide Whatsapp Web Media",
    "description": "This extension hides the media received in the Whatsapp Web page.",
    "version": "1.0",
    
    "browser_action": {
        "default_icon": "icon.png"
    },
    
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ],
    
    "content_scripts": [
        {
            "matches": ["https://web.whatsapp.com/"], 
            "css": ["main.css"]
        }
    ]
}